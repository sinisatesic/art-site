/*here starts code for blob 6*/
#blob6 {
	position: absolute;
	height: 100px;
	width: 100px;
	border-radius: 50%;
	background: black;
	z-index: 2;
	animation: blobbysixth 1s;
	animation-iteration-count: infinite;
}

@keyframes blobbysixth {
	0%{
		transform: translate3d(12em, 8em, 2em); background: #9c1c17;
	}
	25%{
		transform: translate3d(0em, 5em, 1em); background: #bf2121;
	}
	50%{
		transform: translate3d(0em, 2em, 0.5em); background: #e60f00;
	}
	75%{
		transform: translate3d(0em, 5em, 1em); background: #db330d;
	}
	100%{
		transform: translate3d(12em, 8em, 2em); background: #9c1c17;
	}
}

#blob6M {
	position: relative;
	height: 15px;
	width: 30px;
	background: #488924;
	margin: 0 1em 1em 0;
	border-top-left-radius: 200px;
	border-top-right-radius: 200px;
	border-bottom-left-radius: 75px;
	border-bottom-right-radius: 75px;
	margin-left: 30%;
	margin-top: 15%;
	animation: blobsixthmouth 1s;
	animation-iteration-count: infinite;
}

@keyframes blobsixthmouth {
	0%{
		background: #750202;
	}
	25%{
		background: #421111;
	}
	50%{
		background: #2b1010;
	}
	75%{
		background: #421111;
	}
	100%{
		background: #750202;
	}
}

#blob6Leye {
	background: black;
	position: relative;
	width: 10px;
	height: 2px;
	border-top: 0px solid transparent;
	border-right: 9px solid black;
	transform: rotate(10deg);
	margin-left: 15%;
	margin-top: 35%;
}

#blob6Reye {
	background: black;
	position: relative;
	width: 10px;
	height: 2px;
	border-top: 0px solid transparent;
	border-right: 9px solid black;
	transform: rotate(-10deg);
	margin-left: 60%;
	margin-top: -1%;
}

#blob6Liris {
	position: absolute;
	width: 10px;
	height: 10px;
	border-radius: 50%;
	margin-left: 25%;
	margin-top: 20%;
	animation: irisy 2s;
	animation-iteration-count: infinite;
}

#blob6Riris {
	position: absolute;
	width: 10px;
	height: 10px;
	border-radius: 50%;
	margin-left: 25%;
	margin-top: 20%;
	animation: irisy 2s;
	animation-iteration-count: infinite;
}

@keyframes irisy {
	0%{
		background: #f5ec42; transform: scale(0);
	}
	25%{
		background: #fffffc; transform: scale(-.25);
	}
	50%{
		background: #f5ec42; transform: scale(-.75);
	}
	75%{
		background: #fffffc; transform: scale(-.25);
	}
	100%{
		background: #f5ec42; transform: scale(0);
	}
}


