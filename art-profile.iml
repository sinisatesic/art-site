* {
	box-sizing: border-box;
	margin: auto;
}


#whole-page-container {
	width: 100%;
	height: 100%;
	display: grid;
	background: #f6ff4f;
	grid-template-rows: repeat(5, 1fr);
	grid-template-columns: repeat(5, 1fr);

	
}

.first-art {
	margin-top: 1%;
	grid-area: 2 / 1 / 3/ 3;
}

.second-art {
	margin-top: 1%;
	grid-area: 2/ 4/ 3/ 6;
}

.third-art {
	grid-area: 2/ 1/ 4/ 3;
}

.fourth-art {
	grid-area: 2/ 4/ 4/ 6;
}

.fifth-art {
	grid-area: 3/ 2/ 4/ 3;
	border: 15px;
}

.sixth-art {
	grid-area: 3/ 4/ 4/ 5;
	border: 15px;
	animation: clockwise 5s;
	animation-iteration-count: infinite;
}

